@page
@model RegisterModel
@{
    ViewData["Title"] = "Register";
    Layout = null;
}

 
<!DOCTYPE html>
<html lang="en">
<head>
    <style>
        
        
    </style>


    <title>Licensing  | Registration Page</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/css/Plugin/Bootstarp/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/adminlte.min.css" rel="stylesheet" />
</head>
<body class="hold-transition register-page">
    <div class="register-box">
        <div class="register-logo">
            <b>Licensing Main</b>
        </div>
        <div class="container">
            <div class="card">
                <div class="card-body register-card-body">
                    <h4>Create a new account.</h4>
                    <hr />
                    <div asp-validation-summary="All" class="text-danger"></div>
                    <form asp-route-returnUrl="@Model.ReturnUrl" method="post">
                        <div class="input-group mb-4">
                            <div class="input-group-append">
                                <div class="input-group-text">
                                    <span class="fa fa-envelope"></span>
                                    </div>
                                </div>
                                <input asp-for="Input.Email" class="form-control borderwrap" placeholder="EmailId" style=" border-right:-1px !important" />

                            </div>
                        <div class="input-group mb-4">
                            <div class="input-group-append">
                                <div class="input-group-text">
                                    <span class="fa fa-lock"></span>
                                </div>
                            </div>
                            <input asp-for="Input.Password" class="form-control" placeholder="Password" />

                        </div>
                        <div class="input-group mb-4">
                            <div class="input-group-append">
                                <div class="input-group-text">
                                    <span class="fa fa-lock"></span>
                                </div>
                            </div>
                            <input asp-for="Input.ConfirmPassword" class="form-control" placeholder="Confirmpassword" />

                        </div>

                        <button type="submit" class="btn btn-primary">Register</button>
                    </form>
                    <a id="login" asp-page="./Login" class="text-center">I already have an account</a>

                </div>
            </div>
        </div>
    </div>

    <script src="~/js/jquery.js" type="text/javascript"></script>
    <script src="~/css/Plugin/Bootstarp/bootstrap.min.js"></script>
    <script src="~/js/adminlte.min.js"></script>
</body>
</html>





@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
