@model List<ProductImageViewModel>

@{
    ViewData["Title"] = "Downalod Files";
}

<table class="table">
    <tr>
        <th>ProductId  </th>
        <th>FileName </th>
        <th>Customer Name</th>
        <th>Email</th>
        <th>Order Created Date</th>
        <th>Download</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.WCProductId
            </td>
            <td>
                @item.FileName
            </td>

            <td>
                @item.CustomerName
            </td>
            <td>
                @item.Email
            </td>
            <td>
                @item.OrderCreatedDate
            </td>
            <td>
                <a class="fa fa-download" href="@item.FileUrl" target="_blank"></a>
            </td>
        </tr>
    }

</table>