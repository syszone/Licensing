@model List<ProductLicenseViewModel>

@{
    ViewData["Title"] = "Download License Files";
}

<table class="table">
    <tr>
        <th>ProductId </th>
        <th>Product Name</th>
        <th>License Type</th>
        <th>Customer Name</th>
        <th>Email</th>
        <th>Order Created Date</th>
        <th>Download</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            @*<td>
                @item.ProductId
            </td>*@
            <td>
                @item.Name
            </td>
            <td>
                @item.Type
            </td>

            <td>
                @item.CustomerName
            </td>
            <td>
                @item.Email
            </td>
            <td>
                @item.OrderCreatedDate
            </td>
            <td>
                <a class="fa fa-download" href="@item.LicenseURL" target="_blank"></a>
            </td>
        </tr>
    }

</table>