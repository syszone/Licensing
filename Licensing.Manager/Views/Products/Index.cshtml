@model ProductResponse
@{
    ViewData["Title"] = "Create Product";

             

}



<style>
    #allcat {
        height: 390px;
        overflow-y: scroll;
    }

    .divProductInfo .tab-content.link-wrap {
        border: none;
    }

    .ui-autocomplete-loading {
        background: white url('/images/loading.gif') right center no-repeat;
        background-size: 50px;
    }

    .downloableShow {
        position: absolute;
        right: 10px;
    }
</style>

<div class="loader" id="loaderDiv" style="display:none">
    <img src="~/images/Rolling-1s-200px.svg" alt="gif" />
</div>
<div class="">
    <form>
        
        <input type="hidden" id="hdnProductId" value="@ViewBag.ProductId" />
        <input type="hidden" id="hdnWcProductId" value="@ViewBag.WcProductId" />
        <div class="row">
            <div class="col-4">
                <div class="form-group">
                    <label>Product Name</label>
                    <input type="text" class="form-control form-control-sm" id="productName">
                </div>
                <div class="form-group">
                    <label>Product Description</label>
                    <textarea class="form-control form-control-sm" rows="5" id="txtproductDescription"></textarea>
                </div>
                <div class="form-group">
                    <label>Product Short Description</label>
                    <textarea class="form-control form-control-sm" rows="3" id="txtproductshortDescription"></textarea>
                </div>
                <div class="form-group">
                    <label for="Select Duration">Product Features</label>
                    <select class="form-control" id="ddlFeatures" multiple="multiple" name="ddlFeatures"></select>

                </div>


                @*<div class="form-group">
                        <label>Product Images</label>
                        <input name="file" type="file" id="dropProductImages" multiple required accept=".png,.jpg,.jpeg" />
                    </div>*@
            </div>
            <div class="col-8">
                <div class="divHeader">
                    <label>Product Category</label>
                </div>
                <div class="clearfix"></div>
                <div id="allcat" class="tab-pane active">
                    <div id="jstreeAllCategory" class="demo" style="margin-top: 2em;"></div>
                </div>
            </div>
        </div>

        <label class="mr-2" style="font-weight:bold">
            Simple Product
        </label>
        <div class="fs-14">
            <div class="card">
                <p class="tab-header">
                    <div class="container" id="downloadableChk">
                        <label class="custom-checkbox">
                            Downloadable:
                            <input type="checkbox" id="chkDownloadable" />
                            <span class="checkmark"></span>
                        </label>
                    </div>
                </p>
                <div class="row border-top no-gutters tab-wrap">
                    <div class="col-4">
                        <!-- Nav tabs -->
                        <ul class="nav nav-tabs flex-column text-capitalize" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" id="tabGeneral" data-toggle="tab" href="#general">general</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="tab" href="#inventory">inventory</a>
                            </li>
                            @*<li class="nav-item">
                                    <a class="nav-link" data-toggle="tab" href="#shipping">shipping</a>
                                </li>*@

                            <li class="nav-item">
                                <a class="nav-link" data-toggle="tab" href="#linked">linked products</a>
                            </li>
                            @*<li class="nav-item">
                                    <a class="nav-link" data-toggle="tab" href="#components" id="tabcomponents">components</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" data-toggle="tab" href="#scenarios" id="tabscenarios">scenarios</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" data-toggle="tab" href="#bundle" id="tabbundle">Bundle Products/Composite Product</a>
                                </li>*@
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="tab" href="#attributes">Attributes products</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="tab" href="#variations">Variations</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="tab" href="#advanced">advanced products</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="tab" href="#getmore">getmore products</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="tab" href="#custom">custom products</a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-8">
                        <!-- Tab panes -->
                        <div class="tab-content">
                            <div id="general" class="tab-pane active">
                                <div class="container p-3">
                                    <div class="form-group">
                                        <label for="rPrice">Regular price ($)</label>
                                        <input type="number" value="0" class="form-control form-control-sm" id="txtrRegularPrice">
                                    </div>
                                    <div class="form-group">
                                        <label for="sPrice">Sale price ($)</label>
                                        <input type="number" value="0" class="form-control form-control-sm" id="txtsalePrice">
                                        <a class="ml-2" id="hrefschedule" href="javascript:void(0)">Schedule</a>
                                    </div>
                                    <div class="form-group" id="divsalepricedate" style="display:none">
                                        <label for="sPrice">Sale price Date</label>
                                        <input type="text" class="form-control form-control-sm mb-3" name="datePicker" id="txtsalepricefromdate" placeholder="FROM... YYYY-MM-DD">

                                        <input type="text" class="form-control form-control-sm" name="datePicker" id="txtsalepricetodate" placeholder="FROM.. YYYY-MM-DD">
                                        <a class="ml-2" id="hrefcancel" href="javascript:void(0)">Cancel</a>

                                    </div>


                                </div>

                                <div class="container p-3" style="display:none" id="divDownloadable">
                                    <hr />
                                    <div class="form-group">
                                        <label class="radio">
                                            Add Link
                                            <input type="radio" name="imageOption" value="AddLink" checked />

                                        </label>
                                        <label class="radio">
                                            Choose File
                                            <input type="radio" name="imageOption" value="ChooseFile" />

                                        </label>
                                    </div>
                                    <div class="form-group" style="display:none" id="divchoosefiles">
                                        <label for="rPrice">Downloadable files</label>
                                        <input type="file" id="DropdownloadableImage" multiple>

                                    </div>
                                     
                                    <div class="AddFiles">

                                    </div>
                                    <div class="form-group">
                                        <label for="rPrice">Download limit</label>
                                        <input type="number" value="0" class="form-control form-control-sm" id="txtDownloadLimit" placeholder="Unlimited">
                                        <p>Leave blank for unlimited re-downloads.</p>
                                    </div>
                                    <div class="form-group">
                                        <label for="sPrice">Download expiry</label>
                                        <input type="number" value="0" class="form-control form-control-sm" id="txtdownloadExpiry" placeholder="Never">
                                        <p>Enter the number of days before a download link expires, or leave blank.</p>
                                    </div>
                                    <div class="form-group" id="divsalepricedate" style="display:none">
                                        <label for="sPrice">Sale price Date</label>
                                        <input type="text" class="form-control form-control-sm" name="datePicker" id="txtsalepricefromdate" placeholder="FROM... YYYY-MM-DD">
                                        <a class="ml-2" id="hrefcancel" href="javascript:void(0)">Cancel</a>
                                        <p class="form-group mb-0 date-wrap">
                                            <input type="text" class="form-control form-control-sm" name="datePicker" id="txtsalepricetodate" placeholder="FROM.. YYYY-MM-DD">
                                        </p>
                                    </div>
                                </div>
                                <hr />
                                <div class="container p-3">
                                    <div class="form-group">
                                        <label>Tax status</label>
                                        <select class="form-control form-control-sm" id="ddltaxStatus">
                                            <option selected="selected" value="taxable">Texable</option>
                                            <option value="shipping">Shipping Only</option>
                                            <option value="none">None</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label>Tax class</label>
                                        <select class="form-control form-control-sm" id="ddlTaxClass">
                                            <option>Standard</option>
                                            <option>Reduced Rate</option>
                                            <option>Zero Rate</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div id="inventory" class="tab-pane fade">
                                <div class="container p-3">
                                    <div class="form-group">
                                        <label for="sku">SKU</label>
                                        <input type="text" class="form-control form-control-sm" id="txtsku" value="@DateTime.Now.Ticks">
                                    </div>
                                    <div class="form-group">
                                        <label for="stock">Manage stock?</label>
                                        <label class="custom-checkbox">
                                            <input type="checkbox" id="chkenablestock" />
                                            Enable stock management at product level
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="form-group" id="divstockquantity" style="display:none">
                                        <label for="stockquantity">Stock Quantity</label>
                                        <input type="number" class="form-control form-control-sm mb-2" id="txtStockQuantity" value="0">

                                        <label>Allow Backorders?</label>
                                        <select class="form-control form-control-sm mb-2" id="ddlAllowbackorders">
                                            <option>Do not Allow</option>
                                            <option>Allow,but Notify Customer</option>
                                            <option>Allow</option>
                                        </select>

                                        <label>Low stock threshold</label>
                                        <input type="number" class="form-control form-control-sm mb-2" id="txtlowstockthreshold" placeholder="Store-wide thresold(2)">

                                    </div>

                                    <div class="form-group" id="divstockstatus">
                                        <label>Stock status</label>
                                        <select class="form-control form-control-sm" id="ddlstockstatus">
                                            <option selected="selected" value="instock">In stock</option>
                                            <option value="outofstock">Out of stock</option>
                                            <option value="onbackorder">On backorder</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="container p-3">
                                    <div class="form-group">
                                        <label for="stock">Sold individually</label>
                                        <label class="custom-checkbox mr-3">
                                            <input type="checkbox" id="chksolIndividually" />
                                            <span class="checkmark"></span>
                                            Enable this to only allow one of this item to be bought in a single order
                                        </label>
                                    </div>
                                </div>
                            </div>

                            <div id="scenarios" class="tab-pane fade">
                                <div class="container p-3 text-center">
                                    <p class="scenario-icon mb-0">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="100" height="100" fill="#ddd" class="bi bi-arrow-repeat" viewBox="0 0 16 16">
                                            <path d="M11.534 7h3.932a.25.25 0 0 1 .192.41l-1.966 2.36a.25.25 0 0 1-.384 0l-1.966-2.36a.25.25 0 0 1 .192-.41zm-11 2h3.932a.25.25 0 0 0 .192-.41L2.692 6.23a.25.25 0 0 0-.384 0L.342 8.59A.25.25 0 0 0 .534 9z" />
                                            <path fill-rule="evenodd" d="M8 3c-1.552 0-2.94.707-3.857 1.818a.5.5 0 1 1-.771-.636A6.002 6.002 0 0 1 13.917 7H12.9A5.002 5.002 0 0 0 8 3zM3.1 9a5.002 5.002 0 0 0 8.757 2.182.5.5 0 1 1 .771.636A6.002 6.002 0 0 1 2.083 9H3.1z" />
                                        </svg>
                                    </p>
                                    <h3>Scenarios</h3>
                                    <p class="text-muted">First, <a href="#" target="_blank">create some Components</a> by navigating to the Components tab. <br>Then, return here to <a href="#" target="_blank">add Scenarios</a>.</p>
                                </div>
                            </div>
                            <div id="shipping" class="tab-pane fade">
                                <div class="container p-3">
                                    <div class="form-group">
                                        <label>Shipping class</label>
                                        <ul class="list-unstyled layout_wrap ship mb-0">
                                            <li>
                                                <label class="custom_radio">
                                                    <svg xmlns="http://www.w3.org/2000/svg" version="1.0" width="80" height="80" viewBox="0 0 512.000000 410.000000" preserveAspectRatio="xMidYMid meet">

